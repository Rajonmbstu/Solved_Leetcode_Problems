class Solution {
public:
    int maximumEnergy(vector<int>& e, int k) {
        int res=-1000000;
        int n=e.size();
        for(int i=1;i<=k;i++){
            int ans=0;
            int mx=-1000000;
            for(int j=n-i;j>=0;j-=k){
                ans+=e[j];
                mx=max(mx,ans);    

            }
            res=max(res,mx);
        }
        return res;

        
    }
};
